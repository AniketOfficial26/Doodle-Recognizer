# Example environment variables for Refactored Backend
# Copy to .env and fill in secrets. Do NOT commit your real .env

# Google Gemini
GEMINI_API_KEY=

# Optional: CORS origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000,http://localhost:3001,http://127.0.0.1:3001

# Optional: Stability AI (if you use image generation)
STABILITY_API_KEY=
